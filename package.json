{
  "name": "tree-sitter-cadence",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "types": "bindings/node",
  "scripts": {
    "test": "tree-sitter test && tree-sitter parse -q 'examples/**/*.cdc'",
    "install": "node-gyp-build",
    "prebuildify": "prebuildify --napi --strip",
    "lint": "eslint grammar.js",
    "lint-fix": "eslint grammar.js --fix",
    "build-wasm": "tree-sitter build --wasm"
  },
  "keywords": [
    "parser",
    "cadence"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bluesign/cadence-tree-sitter.git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@onflow/cadence-parser": "^0.42.1",
    "cadence-parser": "^1.0.1",
    "node-addon-api": "^7.1.0",
    "node-gyp-build": "^4.8.0"
  },
  "peerDependencies": {
    "tree-sitter": "^0.25.0"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "devDependencies": {
    "eslint": "^8.47.0",
    "eslint-config-google": "^0.14.0",
    "tree-sitter-cli": "^0.22.2",
    "prebuildify": "^6.0.0"
  },
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**"
  ],
  "tree-sitter": [
    {
      "scope": "source.cadence",
      "injection-regex": "cadence",
      "file-types": [
        "cdc"
      ],
      "highlights": [
        "queries/cadence/highlights.scm"
      ]
    }
  ]
}
